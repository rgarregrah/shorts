{
    "compilerOptions": {
        "target": "es2016",
        "module": "commonjs",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "rootDir": "src",
        "typeRoots": ["node_modules/@types"],
        "skipLibCheck": true /* Skip type checking all .d.ts files. */
    },
    "exclude": ["node_modules"]
}
